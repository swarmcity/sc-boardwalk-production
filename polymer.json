{
  "entrypoint": "index.html",
  "shell": "src/swarm-city.html",
    "fragments": [
    "src/pages/page-welcome.html",
    "src/pages/page-language.html",
    "src/pages/page-location.html",
    "src/pages/page-hashtag.html",
    "src/pages/page-new-here.html",
    "src/pages/page-hashtag-list.html",
    "src/pages/page-choose-username.html",
    "src/pages/page-private-key.html",
    "src/pages/page-choose-avatar.html",
    "src/pages/page-resize-avatar.html",
    "src/pages/page-json-file.html",
    "src/pages/page-view404.html",
    "src/pages/page-stop-signup.html",
    "src/pages/page-password-warning.html",
    "src/pages/page-choose-password.html"
  ],
  "sources": [
    "src/**/*",
    "src/pages/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "node_modules/redux/dist/redux.min.js",
    "src/text-translations.json",
    "src/swarm-city.min.js"
  ],
  "builds": [
  {
    "name": "production",
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": false},
    "bundle": true,
    "addServiceWorker": true,
    "addPushManifest": true,
    "insertPrefetchLinks": true
  }
],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}
